generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  firstName String   @map("first_name")
  lastName  String   @map("last_name")
  photoUrl  String?  @map("photo_url")
  password  String
  createdAt DateTime @default(now())
}

enum Role {
  ADMIN
  USER
}
